# Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# Alpaca Trading API
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here

# LangSmith (Optional - for tracing)
LANGSMITH_TRACING=true
LANGSMITH_PROJECT="alpaca-agent"
LANGSMITH_WORKSPACE_ID=your_workspace_id
LANGSMITH_API_KEY=your_langsmith_api_key
LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# Google AI (for LLM)
GOOGLE_API_KEY=your_google_api_key

# LangChain (if using)
LANGCHAIN_API_KEY=your_langchain_api_key

# HyperBrowser (for web scraping)
HYPERBROWSER_API_KEY=your_hyperbrowser_api_key

# Astra DB (for vector storage)
ASTRA_DB_API_ENDPOINT=your_astra_db_endpoint
ASTRA_DB_APPLICATION_TOKEN=your_astra_db_token

# Composio (for tool integrations)
COMPOSIO_API_KEY=your_composio_api_key

# Project Configuration
PROJECT_ID=your_project_id
ORG_ID=your_org_id
ORG_MEMBER_EMAIL=your_email@example.com
USER_ID=your_user_id
PLAYGROUND_TEST_USER_ID=your_test_user_id

# Trading Configuration
AUTO_START_TRADING=false